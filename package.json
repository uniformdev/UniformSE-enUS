    "@netlify/functions": "1.4.0",
    "@netlify/plugin-nextjs": "4.30.4",
    "@next/bundle-analyzer": "13.1.6",
    "@types/cookie": "0.5.1",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "@uniformdev/cli": "^18.7.0",
    "autoprefixer": "10.4.13",
    "dotenv": "16.0.3",
    "netlify-cli": "12.10.0",
    "node-fetch": "2.6.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "^2.8.4",
