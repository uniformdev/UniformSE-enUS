    "@uniformdev/canvas-sanity": "^18.8.0",
    "@uniformdev/canvas-strapi": "^17.1.0",
    "@uniformdev/context": "^18.8.0",
    "@uniformdev/context-devtools": "^18.8.0",
    "@uniformdev/context-gtag": "^18.8.0",
    "@uniformdev/context-next": "^18.8.0",
    "@uniformdev/context-react": "^18.8.0",
    "@uniformdev/project-map": "^18.8.0",
    "algoliasearch": "^4.14.3",
    "analytics": "0.8.1",
    "contentful": "^9.3.2",
    "contentstack": "^3.16.0",
    "cookie": "0.5.0",
    "follow-redirects": "^1.15.2",
